<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>HotelApp</display-name>
  <servlet>
	<servlet-name>RegisterServlet</servlet-name>
	<servlet-class>controller.userInsert</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>controller.loginverify</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>Booking</servlet-name>
	<servlet-class>controller.bookingRoom</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>Logout</servlet-name>
	<servlet-class>controller.logout</servlet-class>
  </servlet>


  <servlet-mapping>
	<servlet-name>RegisterServlet</servlet-name>
	<url-pattern>/View/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/View/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>Booking</servlet-name>
	<url-pattern>/View/book</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
	<servlet-name>Logout</servlet-name>
	<url-pattern>/View/logout</url-pattern>
  </servlet-mapping>
</web-app>